# Elementor Extractor Environment Variables
# Copy this file to .env and fill in your values

# WordPress Connection
ELEMENTOR_WP_URL=https://example.com
ELEMENTOR_WP_USERNAME=admin
ELEMENTOR_WP_PASSWORD=your-application-password-here

# Output Settings
ELEMENTOR_OUTPUT_DIR=./output

# Logging
ELEMENTOR_LOG_LEVEL=info
# ELEMENTOR_LOG_FILE=./logs/extractor.log

# Extraction Options
ELEMENTOR_CONTINUE_ON_ERROR=true
ELEMENTOR_VALIDATE_SCHEMAS=true
